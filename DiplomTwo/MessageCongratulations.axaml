<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="350"
        x:Class="DiplomTwo.MessageCongratulations" Width="500" Height="350"
		MaxWidth="500" MinWidth="500" MaxHeight="350" MinHeight="350"
        Title="MessageCongratulations">
	<StackPanel>
		<TextBlock Margin="0,20,0,0"  FontSize="20" Width="300" TextAlignment="Center" TextWrapping="Wrap" HorizontalAlignment="Center">Поздравляем! 
		Вы получили новое достижение!</TextBlock>
		<Image Margin="10" Width="150" Height="150" Name="imageForA" VerticalAlignment="Center" HorizontalAlignment="Center"></Image>
		<TextBlock Name="nameA" Width="400" TextAlignment="Center" TextWrapping="Wrap" HorizontalAlignment="Center" FontSize="16" Margin="0,10,0,0"></TextBlock>
	</StackPanel>
</Window>
